<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <Color x:Key="HighlightDarkColor">#FF389F00</Color>
    <Color x:Key="HighlightLightColor">#FF59B228</Color>

    <Color x:Key="AccentDarkColor">#84BA4F</Color>
    <Color x:Key="AccentContrastColor">#6648cb00</Color>
    
    <Color x:Key="AccentColor">#CC42871C</Color>
    <!--60%-->
    <Color x:Key="AccentColor2">#995C8D42</Color>
    <!--40%-->
    <Color x:Key="AccentColor3">#6648cb00</Color>
    <!--20%-->
    <Color x:Key="AccentColor4">#3348cb00</Color>

    <LinearGradientBrush x:Key="BrandingBrush" 
                         EndPoint="0.001,0.5" 
                         StartPoint="1.002,0.5">
        <GradientStop Color="{DynamicResource HighlightDarkColor}" 
                      Offset="0" />
        <GradientStop Color="#FF98370F" 
                      Offset="1" />
    </LinearGradientBrush>

    <SolidColorBrush x:Key="AccentColorBrush"
                     Color="{StaticResource AccentColor}" />

    <SolidColorBrush x:Key="AccentSelectedColorBrush"
                     Color="White" />
    <Color x:Key="IdealForegroundColor">White</Color>
    <SolidColorBrush x:Key="IdealForegroundColorBrush" Color="{StaticResource IdealForegroundColor}"/>
</ResourceDictionary>